        "feh",
        "rofi",
        "duf",
        "ncdu",
        "udisks2",
        "docker",
        "fzf",
        "lshw",
        "mpv",
        "alacritty",
        "tmux",
        "arandr",
        "xorg-xinit",
        "xorg",
        "autorandr",
        "openssh",
        "openssl",
        "zlib",
        "xz",
        "neofetch",
        "cmatrix",
        "htop",
        "mc",
        "ranger",
        "pass",
        "xclip",
        "wmname",
        "yt-dlp",
        "zsh"
